#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules/nanoid/bin/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules/nanoid/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/desktop/short-url-maker/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules/nanoid/bin/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules/nanoid/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/nanoid@5.1.5/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules/.pnpm/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/short-url-maker/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/Desktop/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Anjan Chakraborty/desktop/short-url-maker/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nanoid.js" "$@"
else
  exec node  "$basedir/../../bin/nanoid.js" "$@"
fi
